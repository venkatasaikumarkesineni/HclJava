package com.hcl.boxing;

public class OverLoadDemo1 
{
	public void show(int x)
	{
		System.out.println("showmethos wrt to int" + x);
	}
	public void show(int x, int y)
	{
		System.out.println("showmethos wrt to int" + x + y);
	}
	public void show(double x)
	{
		System.out.println("showmethod wrt double" +x);
	}
	public void show(String x,String y)
	{
		System.out.println("show methos wrt string"+x+y);
	}
	public void show(float x)
	{
		System.out.println("show methpd wrt float"+x);
	}
	public static void main(String[] args) {
		OverLoadDemo1 ob=new OverLoadDemo1();
		ob.show("sai","siri");
		ob.show(1,2);
	}
	

}
