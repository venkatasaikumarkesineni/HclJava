package com.hcl.junit;

public class Data {
	public int minArray(int[] x){
		int min=x[1];
		for(int i=1;i<x.length;i++)
		{
			if(min>x[i])
				min=x[i];
		}
		return min;
	}
	public int max3(int a,int b,int c) { 
		int m=a;
		if(m<b){
			m=b;
		}
		if(m<c){
			m=c;
		}
		return m;
	}
	public String sayHello(){
		return "welcome to junit";
	}
public int sum(int a,int b){
	return a+b;
}
}
